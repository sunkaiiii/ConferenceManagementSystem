<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<fx:root type="javafx.scene.layout.StackPane" style="-fx-background-color: #FFFFFF"
         xmlns="http://javafx.com/javafx/11.0.1"
         xmlns:fx="http://javafx.com/fxml/1"
         prefWidth="Infinity"
>
    <Label fx:id="title" alignment="CENTER" prefWidth="Infinity" style="-fx-font-weight: bold"
           textFill="black">
        <font>
            <Font size="24.0"/>
        </font>
        <padding>
            <Insets bottom="16" top="16"/>
        </padding>
    </Label>

    <StackPane prefWidth="Infinity" alignment="CENTER_RIGHT">
        <MenuButton fx:id="menuButton" alignment="CENTER_RIGHT">
            <items>
                <MenuItem fx:id="logOut" text="Log out"/>
            </items>
        </MenuButton>
    </StackPane>

</fx:root>
